---
import Layout from '../layouts/Layout.astro';
import Nav from '../components/sections/Nav.astro';
import Hero from '../components/sections/Hero.astro';
import Services from '../components/sections/Services.astro';
import About from '../components/sections/About.astro';
import CTA from '../components/sections/CTA.astro';
import Process from '../components/sections/Process.astro';
import FAQ from '../components/sections/FAQ.astro';
import Contact from '../components/sections/Contact.astro';
import Footer from '../components/sections/Footer.astro';

export interface Link {
  text: string;
  href: string;
}

const links = [
  { text: 'Services', href: 'test' },
  { text: 'About', href: 'test' },
  { text: 'Process', href: 'test' },
  { text: 'FAQ', href: 'test' },
  { text: 'Contact', href: 'test' },
];
---

<Layout title='Elev Equity'>
  <main>
    <Nav links={links} />
    <Hero />
    <Services />
    <About />
    <CTA />
    <Process />
    <FAQ />
    <Contact />
    <Footer links={links} />
  </main>
</Layout>
